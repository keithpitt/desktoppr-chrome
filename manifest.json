{
  "name": "Desktoppr",
  "version": "0.0.1",
  "description": "Discover awesome new wallpapers by randomly displaying them on each new tab.",
  "permissions": ["https://api.desktoppr.co/"],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}
